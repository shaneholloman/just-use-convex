{
  "name": "just-use-convex-agent-worker-barrel",
  "main": "src/index.ts",
  "compatibility_date": "2026-01-20",
  "compatibility_flags": [
    "nodejs_compat",
    "nodejs_compat_populate_process_env"
  ],
  "vars": {
    "SANDBOX_ROOT_DIR": "/workspace",
    "NODE_ENV": "production"
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "agentWorker",
        "class_name": "AgentWorker",
        "script_name": "just-use-convex-agent-worker-barrel"
      },
      {
        "name": "Sandbox",
        "class_name": "Sandbox"
      }
    ]
  },
  "vectorize": [
    {
      "binding": "VECTORIZE_CHAT_MESSAGES",
      "index_name": "chat-messages",
      "remote": true
    }
  ],
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": [
        "AgentWorker"
      ],
      "new_classes": [
        "Sandbox"
      ]
    }
  ],
  "workflows": [
    {
      "name": "scheduled-job-workflow",
      "binding": "SCHEDULED_JOB_WORKFLOW",
      "class_name": "ScheduledJobWorkflow",
      "script_name": "just-use-convex-agent-worker-barrel"
    }
  ],
  "containers": [
    {
      "class_name": "Sandbox",
      "image": "docker.io/cloudflare/sandbox:0.7.0"
    }
  ]
}
