{
  "name": "just-use-convex-agent-worker-barrel",
  "main": "src/index.ts",
  "compatibility_date": "2026-02-12",
  "compatibility_flags": [
    "nodejs_compat",
    "nodejs_compat_populate_process_env"
  ],
  "vars": {
    "NODE_ENV": "production",
    "DAYTONA_TARGET": "us",
    "DEFAULT_MODEL": "openai/gpt-5.2-chat",
    "MAX_BACKGROUND_DURATION_MS": "3600000"
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "agentWorker",
        "class_name": "AgentWorker",
        "script_name": "just-use-convex-agent-worker-barrel"
      }
    ]
  },
  "vectorize": [
    {
      "binding": "vectorizeChatMessages",
      "index_name": "chat-messages",
      "remote": true
    }
  ],
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": [
        "AgentWorker"
      ],
      "new_classes": []
    }
  ]
}
